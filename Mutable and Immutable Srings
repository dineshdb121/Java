// Mutable and Immutable Srings

//Immutable

public class Alien {

	public static void main(String[] args) {

		String a = new String("Sachin") ;
		a.concat("Tendulkar");
		System.out.println(a);
		
	}

}
//op Sachin


// Mutable

public class Alien {

	public static void main(String[] args) {

		StringBuffer a = new StringBuffer("Sachin") ;
		a.append("Tendulkar");
		System.out.println(a);
	}
}
// op SachinTendulkar
