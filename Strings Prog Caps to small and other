// Small to caps
public class Stringsprog {
		
	public static void main(String[] args) {
		
		char ch = 'a';
		ch=(char) (ch-32);
		System.out.println(ch);
	
	}

}

// Caps to small

public class Stringsprog {
		
	public static void main(String[] args) {
		
		char ch = 'A';
		ch=(char) (ch+32);
		System.out.println(ch);
	
	}

}
