
public class LaunchFE {
	
	public static void main(String[] args) {
		
		int arr[] = {10,20,30,40};
		
		for (int element:arr)
		{
			System.out.print(element + " ");
		}
	}

}

//2D Array For Each loop
public class LaunchFE {
	
	public static void main(String[] args) {
		
		int arr[][] = {{10,20,30,40},{10,20,30,40}};
		
		for (int ar[]:arr)
		{
			for(int element:ar)
			{
			System.out.print(element + " ");
			}
		}
	}

}

