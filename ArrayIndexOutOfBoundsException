
public class LaunchAA {

	public static void main(String[] args) {
		
		int a [] = new int [3];
		
		for(int i=0; i<4; i++ )
		{
			System.out.println(a[i]);
		}

	}

}

//Without Array Index Out Of Bounds Exception

public class LaunchAA {

	public static void main(String[] args) {
		
		int a [] = new int [3];
		
		for(int i=0; i<3; i++ )
		{
			System.out.println(a[i]);
		}

	}

}

