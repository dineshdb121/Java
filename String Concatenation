
public class Launch6 {

	public static void main(String[] args) {

		String S1 = "Telusko";
		String S2 = "iNeuron";
		String S3 = "Telusko" + "iNeuron"; // Here Memory is allocated in SCP
		
		String S4 = S1 + S2 ; // Here Memory is allocated in Heap Area Because References are Different
		String S5 = S1 + "iNeuron";
		
		System.out.println(S3 == S4);
		
				
	}

}


public class Launch7 {

	public static void main(String[] args) {

		String s1 = "Telusko" + "iNeuron";
		System.out.println(s1); //TeluskoiNeuron
		
		String s2 = "Telusko" + 99;
		System.out.println(s2); //Telusko99
		
		String s3 = "Telusko" + 99 + 99 +10;
		System.out.println(s3); //Telusko999910
		
		String s4 =  10 + 4 + "iNeuron";
		System.out.println(s4);//14iNeuron
		
	}

}

