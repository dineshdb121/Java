class Addition
{
	void add(int a, float b)
	{
	float c = a + b;
	System.out.println(c);
	}
	
	float add(float a, int b)
	{
		float c = a+b;
		System.out.println(c);
		return c;
	}
}

public class LaunchCal {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Addition ad= new Addition();
		ad.add(10,20);
	}

}
